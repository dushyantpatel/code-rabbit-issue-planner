{
    "name": "code-rabbit-issue-planner",
    "version": "1.0.0",
    "description": "Agentic issue planner with LLM based action and planning system",
    "main": "src/main.ts",
    "type": "module",
    "scripts": {
        "build": "tsc",
        "clean": "rm -rf node_modules",
        "reinstall": "rm -rf node_modules && npm install",
        "rebuild": "rm -rf node_modules && npm install && npm run build",
        "test": "echo \"Error: no test specified\" && exit 1",
        "start": "node dist/main.js",
        "dev": "tsx src/main.ts",
        "dev:watch": "nodemon -q -e ts,tsx -x tsx src/main.ts",
        "lint": "eslint src --ext .ts,.tsx",
        "lint:fix": "eslint src --ext .ts,.tsx --fix"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/dushyantpatel/code-rabbit-issue-planner.git"
    },
    "author": "Dushyant Patel",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/dushyantpatel/code-rabbit-issue-planner/issues"
    },
    "homepage": "https://github.com/dushyantpatel/code-rabbit-issue-planner#readme",
    "dependencies": {
        "cors": "^2.8.5",
        "express": "^5.1.0",
        "ts-morph": "^25.0.1"
    },
    "devDependencies": {
        "@eslint/js": "^9.26.0",
        "@types/cors": "^2.8.18",
        "@types/express": "^5.0.1",
        "@types/node": "^22.15.18",
        "@typescript-eslint/eslint-plugin": "^8.32.1",
        "@typescript-eslint/parser": "^8.32.1",
        "eslint": "^9.26.0",
        "eslint-config-google": "^0.14.0",
        "eslint-config-prettier": "^10.1.5",
        "eslint-plugin-prettier": "^5.4.0",
        "globals": "^16.1.0",
        "nodemon": "^3.1.10",
        "prettier": "^3.5.3",
        "tsx": "^4.19.4",
        "typescript": "^5.8.3",
        "typescript-eslint": "^8.32.1"
    }
}
