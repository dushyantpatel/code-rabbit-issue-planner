{
    "name": "code-rabbit-issue-planner",
    "version": "1.0.0",
    "description": "Agentic issue planner with LLM based action and planning system",
    "main": "src/main.ts",
    "type": "module",
    "scripts": {
        "build": "tsc",
        "clean": "rm -rf node_modules",
        "reinstall": "rm -rf node_modules && npm install",
        "rebuild": "rm -rf node_modules && npm install && npm run build",
        "test": "ts-mocha -p test/tsconfig.json test/**/*.test.ts --timeout 10000",
        "test:unit": "ts-mocha -p test/tsconfig.json test/unit/**/*.test.ts --timeout 5000",
        "test:integration": "ts-mocha -p test/tsconfig.json test/integration/**/*.test.ts --timeout 10000",
        "test:coverage": "nyc ts-mocha -p test/tsconfig.json test/**/*.test.ts --timeout 10000",
        "test:unit:coverage": "nyc ts-mocha -p test/tsconfig.json test/unit/**/*.test.ts --timeout 5000",
        "test:integration:coverage": "nyc ts-mocha -p test/tsconfig.json test/integration/**/*.test.ts --timeout 10000",
        "start": "node dist/main.js",
        "dev": "tsx src/main.ts",
        "dev:watch": "nodemon -q -e ts,tsx -x tsx src/main.ts",
        "lint": "eslint src --ext .ts,.tsx",
        "lint:fix": "eslint src --ext .ts,.tsx --fix"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/dushyantpatel/code-rabbit-issue-planner.git"
    },
    "author": "Dushyant Patel",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/dushyantpatel/code-rabbit-issue-planner/issues"
    },
    "homepage": "https://github.com/dushyantpatel/code-rabbit-issue-planner#readme",
    "dependencies": {
        "cors": "^2.8.5",
        "express": "^5.1.0",
        "marked": "^15.0.11",
        "ts-morph": "^25.0.1",
        "winston": "^3.17.0"
    },
    "devDependencies": {
        "@eslint/eslintrc": "^3.3.1",
        "@eslint/js": "^9.26.0",
        "@istanbuljs/nyc-config-typescript": "^1.0.2",
        "@types/chai": "^5.2.2",
        "@types/cors": "^2.8.18",
        "@types/express": "^5.0.1",
        "@types/mocha": "^10.0.10",
        "@types/node": "^22.15.18",
        "@types/sinon": "^17.0.4",
        "@types/supertest": "^6.0.3",
        "@typescript-eslint/eslint-plugin": "^8.32.1",
        "@typescript-eslint/parser": "^8.32.1",
        "chai": "^5.2.0",
        "eslint": "^9.26.0",
        "eslint-config-google": "^0.14.0",
        "eslint-config-prettier": "^10.1.5",
        "eslint-plugin-prettier": "^5.4.0",
        "glob": "^11.0.2",
        "globals": "^16.1.0",
        "mocha": "^11.2.2",
        "nodemon": "^3.1.10",
        "nyc": "^17.1.0",
        "prettier": "^3.5.3",
        "rimraf": "^6.0.1",
        "sinon": "^20.0.0",
        "supertest": "^7.1.1",
        "ts-mocha": "^11.1.0",
        "ts-node": "^10.9.2",
        "tsx": "^4.19.4",
        "typescript": "^5.8.3",
        "typescript-eslint": "^8.32.1"
    },
    "overrides": {
        "glob": "^11.0.2",
        "rimraf": "^6.0.1",
        "inflight": "^2.0.1"
    }
}
